{
  "name": "@atjson/editor",
  "version": "0.7.13",
  "description": "AtJSON Editor",
  "main": "dist/commonjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/commonjs/index.d.ts",
  "scripts": {
    "build": "rm -rf dist; tsc -p . & tsc -p . --module ESNext --outDir dist/modules/ --target ES2017; exit 0",
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "prepublish": "npm run build",
    "start": "../../node_modules/.bin/parcel public/index.html",
    "test": "../../node_modules/jest/bin/jest.js packages/$(basename $PWD) --config=../../package.json"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@atjson/document": "^0.7.13",
    "@atjson/hir": "^0.7.13"
  },
  "devDependencies": {
    "parcel-bundler": "^1.6.2",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
