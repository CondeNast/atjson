{
  "name": "@atjson/editor",
  "version": "0.8.11",
  "description": "AtJSON Editor",
  "main": "dist/commonjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/commonjs/index.d.ts",
  "scripts": {
    "build": "rm -rf dist; tsc -p . & tsc -p . --module ESNext --outDir dist/modules/ --target ES2017; exit 0",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "start": "parcel public/index.html",
    "test": "../../../node_modules/.bin/jest packages/@atjson/$(basename $PWD) --config=../../../package.json"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@atjson/document": "0.8.7",
    "@atjson/hir": "0.8.10",
    "@atjson/renderer-commonmark": "0.8.2",
    "node-sass": "^4.9.0"
  },
  "devDependencies": {
    "parcel-bundler": "1.8.1",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
