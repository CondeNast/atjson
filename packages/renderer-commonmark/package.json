{
  "name": "@atjson/renderer-commonmark",
  "version": "0.5.0",
  "description": "A brand new TypeScript library.",
  "main": "dist/commonjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/amd/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "rm -rf dist; tsc -p . --module AMD --outFile dist/amd/index.js; tsc -p . --module CommonJS --outDir dist/commonjs/; tsc -p . --module ESNext --outDir dist/modules/ --target ES2017; exit 0",
    "test": "../../node_modules/jest/bin/jest.js packages/$(basename $PWD) --config=../../package.json"
  },
  "license": "Apache 2.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@atjson/core": "^0.5.0",
    "@atjson/hir": "^0.5.0",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "@atjson/renderer-hir": "^0.5.0"
  }
}
